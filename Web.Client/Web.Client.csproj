<Project Sdk="Microsoft.NET.Sdk.BlazorWebAssembly">

    <PropertyGroup>
        <TargetFramework>net8.0</TargetFramework>
        <Nullable>enable</Nullable>
        <ImplicitUsings>enable</ImplicitUsings>
    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="Blazored.LocalStorage" Version="4.5.0" />
        <PackageReference Include="Blazored.SessionStorage" Version="2.4.0" />
        <PackageReference Include="FuzzySharp" Version="2.0.2" />
        <PackageReference Include="Microsoft.AspNetCore.Components.WebAssembly" Version="8.0.6" />
        <PackageReference Include="Microsoft.AspNetCore.Components.WebAssembly.Authentication" Version="8.0.6" />
        <PackageReference Include="Microsoft.AspNetCore.WebUtilities" Version="8.0.6" />
        <PackageReference Include="Microsoft.Extensions.Http" Version="8.0.0" />
        <PackageReference Include="MudBlazor" Version="7.0.0-rc.1" />
        <PackageReference Include="MudBlazor.Extensions" Version="1.7.88-prev-2406051024-main" />
        <PackageReference Include="MudBlazor.ThemeManager" Version="1.1.0" />
        <PackageReference Include="MudExRichTextEditor" Version="1.0.9" />
    </ItemGroup>

    <ItemGroup>
      <_ContentIncludedByDefault Remove="wwwroot\css\app.css" />
    </ItemGroup>

    <ItemGroup>
      <None Remove="Properties\launchSettings.json" />
    </ItemGroup>

    <ItemGroup>
      <ProjectReference Include="..\Shared\Shared.csproj" />
    </ItemGroup>

    <ItemGroup>
      <AdditionalFiles Include="Feature\Dashboards\Pages\Dashboard.razor" />
      <AdditionalFiles Include="Feature\Register\Page.razor" />
      <AdditionalFiles Include="Feature\UserManagements\Dialog\AddUserDialog.razor" />
      <AdditionalFiles Include="Feature\UserManagements\Dialog\EditUserDialog.razor" />
      <AdditionalFiles Include="Feature\UserManagements\Page\UserManagement.razor" />
      <AdditionalFiles Include="Layout\Appbar.razor" />
      <AdditionalFiles Include="Layout\AppbarButtons.razor" />
      <AdditionalFiles Include="Layout\BlankLayout.razor" />
      <AdditionalFiles Include="Layout\MainLayout.razor" />
      <AdditionalFiles Include="Layout\NavMenu.razor" />
      <AdditionalFiles Include="Shared\NotificationContent\Announcement_v7_In_Development.razor" />
    </ItemGroup>

</Project>
